              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RequestStatus"
                }
              }
            }
          },
          "400": {
            "description": "If the request ID does not match the payload."
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "500": {
            "description": "Internal Server Error"
          },
          "404": {
            "description": "If the event request does not exist."
          }
        }
      },
      "delete": {
        "tags": [
          "EventRequests"
        ],
        "summary": "Deletes an existing event request.",
        "description": "Managers may only delete event requests they created.",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "The unique identifier of the event request.",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "500": {
            "description": "Internal Server Error"
          },
          "204": {
            "description": "Event request deleted successfully."
