    "/api/event-requests/{id}": {
      "put": {
        "tags": [
          "EventRequests"
        ],
        "summary": "Updates the status of an existing event request.",
        "description": "Only administrators are allowed to update event request statuses.\r\nTypical status transitions include approval or rejection.",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "The unique identifier of the event request.",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "requestBody": {
          "description": "The updated event request data.",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UpdateEventRequest"
              }
            },
